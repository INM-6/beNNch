<?xml version="1.0" encoding="UTF-8"?>
<jube>
  <!-- these are the default values for using the local machine. some may be overwritten by specific benchmarks -->
  <parameterset name="machine_params">
    <parameter name="max_cores" type="shell">$(nproc)</parameter>
    <parameter name="output_folder" type="shell">$(pwd)</parameter>
    <parameter name="conda_folder" type="shell">$jube_benchmark_home/environment</parameter>
    <parameter name="batchsystem">bash</parameter>
  </parameterset>
</jube>
